all$pTP53[all$TP53=="Mut"]<-"1Mut"
all$pTP53[all$TP53 %in% c("Wt","WT")]<-"0WT"
all$pKRAS[all$KRAS=="Mut"]<-"1Mut"
all$pKRAS[all$KRAS %in% c("Wt","WT")]<-"0WT"
all$pEGFR[all$EGFR=="Mut"]<-"1Mut"
all$pEGFR[all$EGFR %in% c("Wt","WT")]<-"0WT"

