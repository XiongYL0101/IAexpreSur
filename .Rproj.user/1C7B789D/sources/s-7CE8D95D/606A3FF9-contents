table(g1$pstage1)##IA  845

g1$OS.time<-as.numeric(g1$OS.time)
g1$OS.status<-as.numeric(g1$OS.status)
g1$PFS.time<-as.numeric(g1$PFS.time)
g1$PFS.status<-as.numeric(g1$PFS.status)

