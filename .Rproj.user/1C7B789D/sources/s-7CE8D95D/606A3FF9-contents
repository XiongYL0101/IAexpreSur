par(mar=c(8,5,3,2),cex=1.0)
A<-plot(cal,
        xlab="Predicted Probability",
        ylab="Actual Probability",
        subtitles=FALSE,
        font.lab=2,cex.lab=1,font.axis=2,cex.axis=0.8)
dev.off()
